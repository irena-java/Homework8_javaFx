<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.sample.controller.NotesController">
   <children>
      <TextArea fx:id="noteDescription" layoutX="32.0" layoutY="23.0" prefHeight="200.0" prefWidth="200.0" />
      <Button layoutX="91.0" layoutY="258.0" mnemonicParsing="false" onMouseClicked="#createNote" text="Create" />
      <Button layoutX="217.0" layoutY="258.0" mnemonicParsing="false" onMouseClicked="#deleteNote" text="Delete" />
      <ListView fx:id="listViewSelectAll" layoutX="255.0" layoutY="22.0" prefHeight="200.0" prefWidth="331.0" />
      <Button fx:id="selectAllButton" layoutX="366.0" layoutY="258.0" mnemonicParsing="false" onMouseClicked="#selectAllNotes" text="SelectAll" />
   </children>
</AnchorPane>
